{
  "builds": [{
    "src": "image/wsgi.py",
    "use": "@vercel/python",
    "config": {
"maxLambdaSize": "115mb",
      "runtime": "python3.9"
    }

  }],
  "functions":{
    "*":{
     "memory": 3008,
      "maxDuration": 30
    },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "image/wsgi.py"
    }
  ]
}