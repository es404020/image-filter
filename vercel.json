{
  "builds": [{
    "src": "image/wsgi.py",
    "use": "@vercel/python",
    "config": {
"maxLambdaSize": "115mb",
      "runtime": "python3.9"
    }

  }],
  "functions":{
    "*":{
      "excludeFiles":"image/playground/media/"
    },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "image/wsgi.py"
    }
  ]
}